    "version": "0.2.0",
    "configurations": [
        {
            "type": "android",
            "request": "launch",
            "name": "Launch App",
            "appSrcRoot": "${workspaceRoot}\\android\\app\\src\\main",
            "apkFile": "${workspaceRoot}/build/app/outputs/apk/debug/app-debug.apk",
            "adbPort": 5037
        },
        {
            "name": "tutorialflutter",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "tutorialflutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "tutorialflutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}